*transparent:                     false
*cursorBlink:                     true
*cursorUnderline:                 true
*colorUL:                         #6AC
*cursorColor:                     #F00
*shading:                         30
*mouseWheelScrollPage:            false
*pointerBlank:                    true
*pointerColor:                    #656565
! --- *imFont:                          xft:UbuntuMono Nerd Font:size=12:antialias=true:hinting=true
! --- *imFont:                          xft:Terminus Re33:size=12:antialias=false:hinting=true
